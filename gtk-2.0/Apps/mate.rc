style "mate-menu"
{
    bg[NORMAL]			= "#000000"
	bg[PRELIGHT]		= "#1f1f1f"	
	bg[SELECTED]		= "#004D8C"
	bg[INSENSITIVE]		= "#1f1f1f"
    fg[NORMAL]			= "#ffffff"
    fg[PRELIGHT]		= "#ffffff"
	text[NORMAL]		= @fg_color_dark
		xthickness = 0
		ythickness = 0
} 

widget_class "*n-menu*" style "mate-menu"
widget "*n-menu*" style "mate-menu"
class "*n-menu*" style "mate-menu"
